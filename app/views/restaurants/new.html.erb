<h1>Create new restaurant here</h1>

<div class="container">
  <div class="col-xs-12 col-offset-3">
  <%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category %>
  <%= f.submit "Create Restaurant", class: "btn btn-primary" %>
<% end %>
  </div>
</div>
